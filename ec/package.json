{
	"name": "ec",
	"private": true,
	"engines": {
		"node": "18.x"
	},
	"scripts": {
		"dev": "npm run generate && next dev",
		"dev:mobile": "cross-env BUILD_TARGET=mobile npm run generate && cross-env BUILD_TARGET=mobile next dev",
		"dev:path": "pathpida --watch",
		"build": "npm run generate && next build",
		"build:mobile": "cross-env BUILD_TARGET=mobile npm run generate && cross-env BUILD_TARGET=mobile next build",
		"test:unit": "npm run generate && APP_CONFIG=my-local jest",
		"start": "next start",
		"lint": "next lint && eslint --fix \"{config,tools}/**/*.{ts,tsx}\" && prettier -w . && stylelint --fix 'src/**/*.{css,scss,sass}'",
		"i18n-scan": "cd tools/i18n-scanner && npm start",
		"generate": "npm run generate:config && npm run generate:path",
		"generate:config": "cd tools/config-generator/ && npm run generate",
		"generate:path": "pathpida",
		"find:deadcode": "ts-unused-exports ./tsconfig.json --allowUnusedTypes --excludePathsFromReport=\"/config/;/src/pages/;.stories.\"",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "export NODE_OPTIONS=--openssl-legacy-provider; build-storybook",
		"stylelint-check": "stylelint-config-prettier-check",
		"analyze-bundle:pc": "cross-env ANALYZE=true npm run build",
		"analyze-bundle:mobile": "cross-env ANALYZE=true npm run build:mobile"
	},
